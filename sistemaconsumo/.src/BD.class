conexion.Connect()

$Sql = "select * from inv" & seccion 
$Rec = conexion.Con.exec($sql)
gridView1.Clear
GridView1.rows.count = $Rec.count + 1 
For $linea = 0 To $Rec.Count - 1 Step 1      
If $Rec!cantidadinsumo < "100"
GridView1[$linea, 3].background = Color.Red
